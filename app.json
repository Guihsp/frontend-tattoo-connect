{
  "expo": {
    "name": "frontend-tatto-connect",
    "slug": "frontend-tatto-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/splash.png",
        "backgroundColor": "#1c1c1c"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
